{
  "kind": "collectionType",
  "collectionName": "profiles",
  "info": {
    "singularName": "profile",
    "pluralName": "profiles",
    "displayName": "Profile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "job_title": {
      "type": "string"
    },
    "min_salary": {
      "type": "decimal"
    },
    "experience": {
      "type": "richtext"
    },
    "applications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::application.application",
      "mappedBy": "profile"
    },
    "job_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::job-type.job-type",
      "mappedBy": "profiles"
    }
  }
}
